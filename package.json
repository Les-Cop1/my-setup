    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.23.2",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.5",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
