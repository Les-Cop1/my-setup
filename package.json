{
  "name": "my-setup",
  "version": "0.0.0",
  "description": "Website to manage your setup",
  "private": true,
  "license": "UNLICENSED",
  "repository": "git@github.com:Les-Cop1/my-setup.git",
  "scripts": {
    "build": "craco build && yarn tailwind:build",
    "check-types": "tsc --noemit",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint . && yarn check-types",
    "prepare": "husky install",
    "start": "craco start",
    "setup": "yarn install",
    "tailwind:build": "tailwindcss -i src/assets/styles/index.css -o dist/index.css",
    "tailwind:start": "tailwindcss -i src/assets/styles/index.css -o dist/index.css --watch",
    "test": "craco test"
  },
